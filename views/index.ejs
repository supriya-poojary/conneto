    <script>
        (function () {
            const navEntries = window.performance && window.performance.getEntriesByType ? window.performance.getEntriesByType('navigation') : [];
            const navType = navEntries.length > 0 ? navEntries[0].type : null;
            const isInternal = document.referrer && document.referrer.includes(window.location.host);
            const urlParams = new URLSearchParams(window.location.search);
            const skipParam = urlParams.get('skipLoader');

            if (skipParam === 'true' || (navType === 'navigate' && isInternal)) {
                document.write('<style>.js-preloader { display: none !important; }</style>');
            }
        })();
    </script>
    <div class="loader js-preloader"
        style="display: flex; flex-direction: column; align-items: center; justify-content: center; background: #0F172A; position: fixed; top: 0; left: 0; width: 100%; height: 100%; z-index: 9999; transition: opacity 0.4s cubic-bezier(0.4, 0, 0.2, 1), visibility 0.4s;">
