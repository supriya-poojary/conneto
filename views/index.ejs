            style="font-family:'Space Grotesk',sans-serif;font-size:3.5rem;font-weight:900;color:#fff;letter-spacing:-1px; animation: logo-pop 0.4s cubic-bezier(0.16, 1, 0.3, 1) forwards;">Conn<span
                style="color:#60A5FA;">eto</span></span>
        <div style="display: flex; gap: 10px; margin-top: 15px;">
            <div
                style="width: 8px; height: 8px; background: #60A5FA; border-radius: 50%; animation: dot-pop 0.4s infinite alternate;">
            </div>
            <div
                style="width: 8px; height: 8px; background: #60A5FA; border-radius: 50%; animation: dot-pop 0.4s infinite alternate 0.1s;">
            </div>
            <div
                style="width: 8px; height: 8px; background: #60A5FA; border-radius: 50%; animation: dot-pop 0.4s infinite alternate 0.2s;">
            </div>
        </div>
        <style>
            @keyframes logo-pop {
                from {
                    opacity: 0;
                    transform: scale(0.9);
                }

                to {
                    opacity: 1;
                    transform: scale(1);
                }
            }

            @keyframes dot-pop {
                from {
                    transform: translateY(0);
                }

                to {
                    transform: translateY(-6px);
                    opacity: 0.6;
                }
            }

            .js-preloader.finished {
                opacity: 0;
                visibility: hidden;
            }
        </style>
    </div>
    <script>
        (function () {
            const preloader = document.querySelector('.js-preloader');
            if (!preloader || preloader.style.display === 'none') return;

            window.addEventListener('load', function () {
                setTimeout(() => {
                    preloader.classList.add('finished');
                    setTimeout(() => preloader.style.display = 'none', 300);
                }, 150); // Minimal delay after load
            });
        })();
    </script>
